#include <stdio.h>
#include <cs50.h>

void print(char k, int t);
int main (void)
{
    int n;
    do
    {
        n= get_int("Height: ");
    }while (n<0);
    
    for (int i=0; i<n;i++)
    {
        print('#',i+1);
        printf("\n");
    }
}

void print(char k, int t)
{
    for (int o=0; o<t; o++)
    {
        printf("%c", k);
    }
}
